# üó∫Ô∏è Google Maps Configuration
# Get your API key from: https://console.cloud.google.com/
# Enable: Maps JavaScript API, Places API, Directions API, Geocoding API
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
VITE_GOOGLE_MAPS_LIBRARIES=places,geometry,drawing,visualization

# üîó API Configuration  
VITE_API_URL=http://localhost:8000
VITE_WEBSOCKET_URL=ws://localhost:8000

# üéõÔ∏è Feature Flags
VITE_ENABLE_OFFLINE_MAPS=true
VITE_ENABLE_AR_NAVIGATION=true
VITE_ENABLE_VOICE_COMMANDS=true
VITE_ENABLE_SOCIAL_FEATURES=true
VITE_ENABLE_ANALYTICS=true

# üìä Analytics & Monitoring
VITE_ANALYTICS_ENABLED=true
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_MONITORING_ENDPOINT=https://your-monitoring-service.com/api
VITE_MONITORING_API_KEY=your_monitoring_key_here
VITE_MONITORING_SAMPLE_RATE=1.0

# üîí Security
VITE_JWT_SECRET=your_jwt_secret_key_here
VITE_ENCRYPTION_KEY=your_encryption_key_here

# üåê Optional: Additional Service Integrations
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_token_here
VITE_OPENWEATHER_API_KEY=your_weather_api_key_here
VITE_UBER_CLIENT_ID=your_uber_client_id_here
