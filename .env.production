# PathFinder Pro - Production Environment Variables

# App Configuration
REACT_APP_ENV=production
REACT_APP_VERSION=1.0.0
REACT_APP_API_BASE_URL=https://api.pathfinderpro.com
REACT_APP_APP_NAME="PathFinder Pro"
REACT_APP_DESCRIPTION="Professional navigation app with real-time traffic and offline maps"

# OpenRouteService Configuration
REACT_APP_ORS_API_KEY=your_ors_api_key_here
REACT_APP_ORS_BASE_URL=https://api.openrouteservice.org

# Map Configuration
REACT_APP_DEFAULT_MAP_CENTER_LAT=37.7749
REACT_APP_DEFAULT_MAP_CENTER_LNG=-122.4194
REACT_APP_DEFAULT_ZOOM_LEVEL=13
REACT_APP_MAX_ZOOM_LEVEL=18
REACT_APP_MIN_ZOOM_LEVEL=3

# PWA Configuration
REACT_APP_THEME_COLOR=#1e40af
REACT_APP_BACKGROUND_COLOR=#ffffff
REACT_APP_DISPLAY_MODE=standalone
REACT_APP_ORIENTATION=portrait

# Analytics & Monitoring
REACT_APP_GOOGLE_ANALYTICS_ID=UA-XXXXXXXX-X
REACT_APP_MONITORING_ENDPOINT=https://monitoring.pathfinderpro.com/api
REACT_APP_MONITORING_API_KEY=your_monitoring_api_key_here
REACT_APP_MONITORING_SAMPLE_RATE=0.1
REACT_APP_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true
REACT_APP_ENABLE_PUSH_NOTIFICATIONS=true
REACT_APP_ENABLE_OFFLINE_MAPS=true
REACT_APP_ENABLE_AR_FEATURES=true
REACT_APP_ENABLE_BACKGROUND_LOCATION=true

# Performance Configuration
REACT_APP_ENABLE_SERVICE_WORKER=true
REACT_APP_CACHE_STRATEGY=network-first
REACT_APP_PRELOAD_CRITICAL_RESOURCES=true
REACT_APP_LAZY_LOAD_COMPONENTS=true

# Security Configuration
REACT_APP_ENABLE_CSP=true
REACT_APP_ENFORCE_HTTPS=true
REACT_APP_API_RATE_LIMIT=100
REACT_APP_API_RATE_WINDOW=60000

# CDN Configuration
REACT_APP_CDN_BASE_URL=https://cdn.pathfinderpro.com
REACT_APP_ASSETS_BASE_URL=https://assets.pathfinderpro.com

# Build Configuration
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false
IMAGE_INLINE_SIZE_LIMIT=0
EXTEND_ESLINT=true